  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch api-go",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/api-go/cmd/server/api-go.go",
      "cwd": "${workspaceFolder}/api-go"
    },
    {
      "name": "Goose Debug up-by-one",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/goose/main.go",
      "cwd": "${workspaceFolder}",
      "args": ["up-by-one"]
    }
  ]
}
