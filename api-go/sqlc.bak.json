{
  "version": "2",
  "sql": [
    {
      "engine": "postgresql",
      "queries": "./sql",
      "schema": "../migrations",
      "gen": {
        "go": {
          "package": "dbquery",
          "out": "pkg/sqlgen",
          "sql_package": "pgx/v5",
          "emit_prepared_queries": true,
          "emit_exact_table_names": true,
          "emit_empty_slices": true,
          "emit_json_tags": true,
          "emit_all_enum_values": true,
          "json_tags_case_style": "none",
          "initialisms": [],
          "rename": {
            "id": "Id"
          }
        },
        "json": {
          "out": "assets/sqlc.json"
        }
      }
    }
  ]
}
