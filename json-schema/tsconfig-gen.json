{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "baseUrl": "./",
    "rootDir": "./gen-schema-ts",
    "outDir": "gen-schema-ts-dist",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "strict": true,
    "declaration": true
  },
  "include": ["./gen-schema-ts/*.ts"],
  "exclude": ["node_modules"]
}
