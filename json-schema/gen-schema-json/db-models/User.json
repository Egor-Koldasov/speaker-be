{
  "type": [
    "object"
  ],
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "User",
  "additionalProperties": false,
  "title": "User",
  "properties": {
    "id": {
      "$rel": "./DbId.json"
    },
    "createdAt": {
      "type": "string",
      "description": "ISO 8601 date string"
    },
    "updatedAt": {
      "type": "string",
      "description": "ISO 8601 date string"
    },
    "deletedAt": {
      "type": [
        "string",
        "null"
      ],
      "description": "ISO 8601 date string or null"
    },
    "email": {
      "type": "string",
      "format": "email"
    }
  },
  "required": [
    "id",
    "createdAt",
    "updatedAt",
    "deletedAt",
    "email"
  ]
}