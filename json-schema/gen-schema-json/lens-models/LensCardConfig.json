{
  "type": [
    "object"
  ],
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "LensCardConfig",
  "additionalProperties": false,
  "title": "LensCardConfig",
  "description": "CardConfig with its fieldConfigs loaded",
  "properties": {
    "id": {
      "$ref": "../db-models/./DbId.json"
    },
    "createdAt": {
      "type": "string",
      "description": "ISO 8601 date string"
    },
    "updatedAt": {
      "type": "string",
      "description": "ISO 8601 date string"
    },
    "deletedAt": {
      "type": [
        "string",
        "null"
      ],
      "description": "ISO 8601 date string or null"
    },
    "name": {
      "type": "string",
      "description": "The name of the card config"
    },
    "fieldConfigByName": {
      "type": "object",
      "additionalProperties": {
        "$ref": "../db-models/FieldConfig.json"
      },
      "description": "A map of fieldConfigs with their names as keys",
      "meta": {
        "relation": "ManyToMany"
      }
    }
  },
  "required": [
    "id",
    "createdAt",
    "updatedAt",
    "deletedAt",
    "name",
    "fieldConfigByName"
  ]
}