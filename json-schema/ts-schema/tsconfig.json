{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "baseUrl": "./",
    "rootDir": "./",
    "outDir": "dist",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "strict": true
  },
  "include": ["./**/*.ts"]
}
