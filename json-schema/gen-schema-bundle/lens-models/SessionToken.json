{
  "type": [
    "object"
  ],
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "SessionToken",
  "additionalProperties": false,
  "title": "SessionToken",
  "properties": {
    "id": {
      "type": "string",
      "description": "UUID v7 string"
    },
    "createdAt": {
      "type": "string",
      "description": "ISO 8601 date string"
    },
    "updatedAt": {
      "type": "string",
      "description": "ISO 8601 date string"
    },
    "deletedAt": {
      "type": [
        "string",
        "null"
      ],
      "description": "ISO 8601 date string or null"
    },
    "userId": {
      "type": "string"
    },
    "tokenCode": {
      "type": "string",
      "length": 12
    }
  },
  "required": [
    "id",
    "createdAt",
    "updatedAt",
    "deletedAt",
    "userId",
    "tokenCode"
  ]
}