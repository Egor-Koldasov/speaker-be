{
  "type": [
    "object"
  ],
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "DbModels",
  "additionalProperties": false,
  "title": "DbModels",
  "properties": {
    "ModelBase": {
      "type": [
        "object"
      ],
      "$schema": "http://json-schema.org/draft-07/schema",
      "$id": "DbModelBase",
      "additionalProperties": false,
      "title": "DbModelBase",
      "properties": {
        "id": {
          "$ref": "#/properties/User/properties/id"
        },
        "createdAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "updatedAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "deletedAt": {
          "type": [
            "string",
            "null"
          ],
          "description": "ISO 8601 date string or null"
        }
      },
      "required": [
        "id",
        "createdAt",
        "updatedAt",
        "deletedAt"
      ]
    },
    "User": {
      "type": [
        "object"
      ],
      "$schema": "http://json-schema.org/draft-07/schema",
      "$id": "User",
      "additionalProperties": false,
      "title": "User",
      "properties": {
        "id": {
          "type": "string",
          "$schema": "http://json-schema.org/draft-07/schema",
          "$id": "DbId",
          "title": "DbId",
          "description": "SurrealDb Id string with a format \"Table:uuid\". Where `uuid` is UUID v7 string"
        },
        "createdAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "updatedAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "deletedAt": {
          "type": [
            "string",
            "null"
          ],
          "description": "ISO 8601 date string or null"
        },
        "email": {
          "type": "string",
          "format": "email"
        }
      },
      "required": [
        "id",
        "createdAt",
        "updatedAt",
        "deletedAt",
        "email"
      ]
    },
    "UserSettings": {
      "type": [
        "object"
      ],
      "$schema": "http://json-schema.org/draft-07/schema",
      "$id": "UserSettings",
      "additionalProperties": false,
      "title": "UserSettings",
      "definitions": {
        "foreignLanguages": {
          "type": "array",
          "description": "The list of BCP 47 language tags of the languages foreign to the user that are most commonly used in the learning process. Take this list as a priority when you try to detect the text language of the text foreign to the user. Although it is not guaranteed to completely match the text languages",
          "items": {
            "type": "string"
          }
        },
        "translationLanguage": {
          "type": "string",
          "description": "The BCP 47 language tag of the language that the user wants to translate the foreign text to."
        },
        "nativeLanguages": {
          "type": "array",
          "description": "The list of BCP 47 language tags of the languages native to the user. Take this list as a priority when you try to detect the text language. Although it is not guaranteed to completely match the text languages",
          "items": {
            "type": "string"
          }
        },
        "primaryForeignLanguage": {
          "type": "string",
          "description": "The BCP 47 language tag of the language that the user wants to translate the text to."
        }
      },
      "properties": {
        "id": {
          "$ref": "#/properties/User/properties/id"
        },
        "createdAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "updatedAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "deletedAt": {
          "type": [
            "string",
            "null"
          ],
          "description": "ISO 8601 date string or null"
        },
        "foreignLanguages": {
          "$ref": "#/properties/UserSettings/definitions/foreignLanguages"
        },
        "translationLanguage": {
          "$ref": "#/properties/UserSettings/definitions/translationLanguage"
        },
        "nativeLanguages": {
          "$ref": "#/properties/UserSettings/definitions/nativeLanguages"
        },
        "primaryForeignLanguage": {
          "$ref": "#/properties/UserSettings/definitions/primaryForeignLanguage"
        }
      },
      "required": [
        "id",
        "createdAt",
        "updatedAt",
        "deletedAt",
        "foreignLanguages",
        "translationLanguage",
        "nativeLanguages",
        "primaryForeignLanguage"
      ]
    },
    "SignUpCode": {
      "type": [
        "object"
      ],
      "$schema": "http://json-schema.org/draft-07/schema",
      "$id": "SignUpCode",
      "additionalProperties": false,
      "title": "SignUpCode",
      "properties": {
        "id": {
          "$ref": "#/properties/User/properties/id"
        },
        "createdAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "updatedAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "deletedAt": {
          "type": [
            "string",
            "null"
          ],
          "description": "ISO 8601 date string or null"
        },
        "email": {
          "type": "string",
          "format": "email"
        },
        "code": {
          "type": "string",
          "length": 6
        }
      },
      "required": [
        "id",
        "createdAt",
        "updatedAt",
        "deletedAt",
        "email",
        "code"
      ]
    },
    "SessionToken": {
      "type": [
        "object"
      ],
      "$schema": "http://json-schema.org/draft-07/schema",
      "$id": "SessionToken",
      "additionalProperties": false,
      "title": "SessionToken",
      "properties": {
        "id": {
          "$ref": "#/properties/User/properties/id"
        },
        "createdAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "updatedAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "deletedAt": {
          "type": [
            "string",
            "null"
          ],
          "description": "ISO 8601 date string or null"
        },
        "userId": {
          "type": "string"
        },
        "tokenCode": {
          "type": "string",
          "length": 12
        }
      },
      "required": [
        "id",
        "createdAt",
        "updatedAt",
        "deletedAt",
        "userId",
        "tokenCode"
      ]
    },
    "CardConfig": {
      "type": [
        "object"
      ],
      "$schema": "http://json-schema.org/draft-07/schema",
      "$id": "CardConfig",
      "additionalProperties": false,
      "title": "CardConfig",
      "properties": {
        "name": {
          "type": "string",
          "description": "The name of the card config"
        }
      },
      "required": [
        "name"
      ]
    },
    "FieldConfig": {
      "type": [
        "object"
      ],
      "$schema": "http://json-schema.org/draft-07/schema",
      "$id": "FieldConfig",
      "additionalProperties": false,
      "title": "FieldConfig",
      "properties": {
        "id": {
          "$ref": "#/properties/User/properties/id"
        },
        "createdAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "updatedAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "deletedAt": {
          "type": [
            "string",
            "null"
          ],
          "description": "ISO 8601 date string or null"
        },
        "name": {
          "type": "string",
          "description": "The name of the field defined by user and displayed back to user"
        },
        "valueType": {
          "type": "string",
          "$schema": "http://json-schema.org/draft-07/schema",
          "$id": "FieldConfigValueType",
          "title": "FieldConfigValueType",
          "enum": [
            "Text",
            "Image",
            "Audio"
          ],
          "description": "The type of the value for AI to generate"
        },
        "minResult": {
          "type": "number",
          "default": 1,
          "description": "The minimum number of results for AI to generate. If the AI cannot generate enough results it can return less, but otherwise should match"
        },
        "maxResult": {
          "type": "number",
          "default": 1,
          "description": "The maximum number of results for AI to generate. AI should never generate more than this number of results."
        },
        "prompt": {
          "type": "string",
          "desctiption": "The user prompt for the AI, explaining what the AI should generate."
        }
      },
      "required": [
        "id",
        "createdAt",
        "updatedAt",
        "deletedAt",
        "name",
        "valueType",
        "minResult",
        "maxResult",
        "prompt"
      ]
    },
    "FieldValue": {
      "type": [
        "object"
      ],
      "$schema": "http://json-schema.org/draft-07/schema",
      "$id": "FieldValue",
      "additionalProperties": false,
      "title": "FieldValue",
      "description": "A value result of FieldConfig. Either AI generated or user input.",
      "properties": {
        "id": {
          "$ref": "#/properties/User/properties/id"
        },
        "createdAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "updatedAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "deletedAt": {
          "type": [
            "string",
            "null"
          ],
          "description": "ISO 8601 date string or null"
        },
        "text": {
          "type": "string"
        },
        "fileId": {
          "$ref": "#/properties/User/properties/id"
        },
        "fieldSetId": {
          "$ref": "#/properties/User/properties/id"
        }
      },
      "required": [
        "id",
        "createdAt",
        "updatedAt",
        "deletedAt",
        "text",
        "fileId",
        "fieldSetId"
      ]
    },
    "FieldValueSet": {
      "type": [
        "object"
      ],
      "$schema": "http://json-schema.org/draft-07/schema",
      "$id": "FieldValueSet",
      "additionalProperties": false,
      "title": "FieldValueSet",
      "description": "A set of results of FieldConfig. Either AI generated or user input.",
      "properties": {
        "id": {
          "$ref": "#/properties/User/properties/id"
        },
        "createdAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "updatedAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "deletedAt": {
          "type": [
            "string",
            "null"
          ],
          "description": "ISO 8601 date string or null"
        },
        "fieldConfigId": {
          "$ref": "#/properties/User/properties/id"
        }
      },
      "required": [
        "id",
        "createdAt",
        "updatedAt",
        "deletedAt",
        "fieldConfigId"
      ]
    },
    "RelCardConfigFieldConfig": {
      "type": [
        "object"
      ],
      "$schema": "http://json-schema.org/draft-07/schema",
      "$id": "RelCardConfigFieldConfig",
      "additionalProperties": false,
      "title": "RelCardConfigFieldConfig",
      "description": "Many to many relation between CardConfig and FieldConfig",
      "properties": {
        "cardConfigId": {
          "$ref": "#/properties/User/properties/id"
        },
        "fieldConfigId": {
          "$ref": "#/properties/User/properties/id"
        }
      },
      "required": [
        "cardConfigId",
        "fieldConfigId"
      ]
    }
  },
  "required": [
    "ModelBase",
    "User",
    "UserSettings",
    "SignUpCode",
    "SessionToken",
    "CardConfig",
    "FieldConfig",
    "FieldValue",
    "FieldValueSet",
    "RelCardConfigFieldConfig"
  ]
}