{
  "type": [
    "object"
  ],
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "LensModels",
  "additionalProperties": false,
  "title": "LensModels",
  "properties": {
    "LensCardConfig": {
      "type": [
        "object"
      ],
      "$schema": "http://json-schema.org/draft-07/schema",
      "$id": "LensCardConfig",
      "additionalProperties": false,
      "title": "LensCardConfig",
      "description": "CardConfig with its fieldConfigs loaded",
      "properties": {
        "id": {
          "type": "string",
          "$schema": "http://json-schema.org/draft-07/schema",
          "$id": "DbId",
          "title": "DbId",
          "description": "SurrealDb Id string with a format \"Table:uuid\". Where `uuid` is UUID v7 string"
        },
        "createdAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "updatedAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "deletedAt": {
          "type": [
            "string",
            "null"
          ],
          "description": "ISO 8601 date string or null"
        },
        "name": {
          "type": "string",
          "description": "The name of the card config"
        },
        "promptParameterDefinitions": {
          "type": "array",
          "description": "A list of all the parameter definitions that will be added to each FieldConfig.",
          "items": {
            "type": [
              "object"
            ],
            "$schema": "http://json-schema.org/draft-07/schema",
            "$id": "PromptParameterDefinition",
            "additionalProperties": false,
            "title": "PromptParameterDefinition",
            "examples": [
              {
                "name": "userLearningLanguages",
                "parameterDescription": "A list of the languages that the user is interested in learning"
              },
              {
                "name": "translationLanguage",
                "parameterDescription": "The language that the user wants to translate to"
              }
            ],
            "properties": {
              "name": {
                "type": "string",
                "description": "The programmical name of the parameter to refer"
              },
              "parameterDescription": {
                "type": "string",
                "description": "The description of purpose and structure of the parameter"
              }
            },
            "required": [
              "name",
              "parameterDescription"
            ]
          }
        },
        "prompt": {
          "type": "string",
          "desctiption": "The user prompt for the AI, explaining what the AI should generate."
        },
        "fieldConfigByName": {
          "type": "object",
          "additionalProperties": {
            "type": [
              "object"
            ],
            "$schema": "http://json-schema.org/draft-07/schema",
            "$id": "FieldConfig",
            "additionalProperties": false,
            "title": "FieldConfig",
            "properties": {
              "id": {
                "type": "string",
                "$schema": "http://json-schema.org/draft-07/schema",
                "$id": "DbId",
                "title": "DbId",
                "description": "SurrealDb Id string with a format \"Table:uuid\". Where `uuid` is UUID v7 string"
              },
              "createdAt": {
                "type": "string",
                "description": "ISO 8601 date string"
              },
              "updatedAt": {
                "type": "string",
                "description": "ISO 8601 date string"
              },
              "deletedAt": {
                "type": [
                  "string",
                  "null"
                ],
                "description": "ISO 8601 date string or null"
              },
              "name": {
                "type": "string",
                "description": "The name of the field defined by user and displayed back to user"
              },
              "valueType": {
                "type": "string",
                "$schema": "http://json-schema.org/draft-07/schema",
                "$id": "FieldConfigValueType",
                "title": "FieldConfigValueType",
                "enum": [
                  "Text",
                  "Image",
                  "Audio"
                ],
                "description": "The type of the value for AI to generate"
              },
              "minResult": {
                "type": "number",
                "default": 1,
                "description": "The minimum number of results for AI to generate. If the AI cannot generate enough results it can return less, but otherwise should match"
              },
              "maxResult": {
                "type": "number",
                "default": 1,
                "description": "The maximum number of results for AI to generate. AI should never generate more than this number of results."
              },
              "promptParameterDefinitions": {
                "type": "array",
                "description": "A list of all the parameter definitions that the FieldConfig uses.",
                "items": {
                  "type": [
                    "object"
                  ],
                  "$schema": "http://json-schema.org/draft-07/schema",
                  "$id": "PromptParameterDefinition",
                  "additionalProperties": false,
                  "title": "PromptParameterDefinition",
                  "examples": [
                    {
                      "name": "userLearningLanguages",
                      "parameterDescription": "A list of the languages that the user is interested in learning"
                    },
                    {
                      "name": "translationLanguage",
                      "parameterDescription": "The language that the user wants to translate to"
                    }
                  ],
                  "properties": {
                    "name": {
                      "type": "string",
                      "description": "The programmical name of the parameter to refer"
                    },
                    "parameterDescription": {
                      "type": "string",
                      "description": "The description of purpose and structure of the parameter"
                    }
                  },
                  "required": [
                    "name",
                    "parameterDescription"
                  ]
                }
              },
              "prompt": {
                "type": "string",
                "desctiption": "The user prompt for the AI, explaining what the AI should generate."
              }
            },
            "required": [
              "id",
              "createdAt",
              "updatedAt",
              "deletedAt",
              "name",
              "valueType",
              "minResult",
              "maxResult",
              "promptParameterDefinitions",
              "prompt"
            ]
          },
          "description": "A map of fieldConfigs with their names as keys",
          "meta": {
            "relation": "ManyToMany"
          }
        }
      },
      "required": [
        "id",
        "createdAt",
        "updatedAt",
        "deletedAt",
        "name",
        "promptParameterDefinitions",
        "prompt",
        "fieldConfigByName"
      ]
    },
    "LensCardValue": {
      "type": [
        "object"
      ],
      "$schema": "http://json-schema.org/draft-07/schema",
      "$id": "LensCardValue",
      "additionalProperties": false,
      "title": "LensCardValue",
      "properties": {
        "cardConfigId": {
          "type": "string",
          "$schema": "http://json-schema.org/draft-07/schema",
          "$id": "DbId",
          "title": "DbId",
          "description": "SurrealDb Id string with a format \"Table:uuid\". Where `uuid` is UUID v7 string"
        },
        "fields": {
          "type": "array",
          "items": {
            "id": {
              "type": "string",
              "$schema": "http://json-schema.org/draft-07/schema",
              "$id": "DbId",
              "title": "DbId",
              "description": "SurrealDb Id string with a format \"Table:uuid\". Where `uuid` is UUID v7 string"
            },
            "createdAt": {
              "type": "string",
              "description": "ISO 8601 date string"
            },
            "updatedAt": {
              "type": "string",
              "description": "ISO 8601 date string"
            },
            "deletedAt": {
              "type": [
                "string",
                "null"
              ],
              "description": "ISO 8601 date string or null"
            },
            "fieldConfigId": {
              "type": "string",
              "$schema": "http://json-schema.org/draft-07/schema",
              "$id": "DbId",
              "title": "DbId",
              "description": "SurrealDb Id string with a format \"Table:uuid\". Where `uuid` is UUID v7 string"
            },
            "fieldValues": {
              "type": "array",
              "items": {
                "type": [
                  "object"
                ],
                "$schema": "http://json-schema.org/draft-07/schema",
                "$id": "FieldValue",
                "additionalProperties": false,
                "title": "FieldValue",
                "description": "A value result of FieldConfig. Either AI generated or user input.",
                "properties": {
                  "id": {
                    "type": "string",
                    "$schema": "http://json-schema.org/draft-07/schema",
                    "$id": "DbId",
                    "title": "DbId",
                    "description": "SurrealDb Id string with a format \"Table:uuid\". Where `uuid` is UUID v7 string"
                  },
                  "createdAt": {
                    "type": "string",
                    "description": "ISO 8601 date string"
                  },
                  "updatedAt": {
                    "type": "string",
                    "description": "ISO 8601 date string"
                  },
                  "deletedAt": {
                    "type": [
                      "string",
                      "null"
                    ],
                    "description": "ISO 8601 date string or null"
                  },
                  "text": {
                    "type": "string"
                  },
                  "fileId": {
                    "type": "string",
                    "$schema": "http://json-schema.org/draft-07/schema",
                    "$id": "DbId",
                    "title": "DbId",
                    "description": "SurrealDb Id string with a format \"Table:uuid\". Where `uuid` is UUID v7 string"
                  },
                  "fieldSetId": {
                    "type": "string",
                    "$schema": "http://json-schema.org/draft-07/schema",
                    "$id": "DbId",
                    "title": "DbId",
                    "description": "SurrealDb Id string with a format \"Table:uuid\". Where `uuid` is UUID v7 string"
                  }
                },
                "required": [
                  "id",
                  "createdAt",
                  "updatedAt",
                  "deletedAt",
                  "text",
                  "fileId",
                  "fieldSetId"
                ]
              }
            }
          }
        }
      },
      "required": [
        "cardConfigId",
        "fields"
      ]
    }
  },
  "required": [
    "LensCardConfig",
    "LensCardValue"
  ]
}