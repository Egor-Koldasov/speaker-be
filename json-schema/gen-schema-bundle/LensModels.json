      "properties": {
        "id": {
          "type": "string",
          "description": "UUID v7 string"
        },
        "createdAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "updatedAt": {
          "type": "string",
          "description": "ISO 8601 date string"
        },
        "deletedAt": {
          "type": [
            "string",
            "null"
          ],
          "description": "ISO 8601 date string or null"
        }
      },
      "required": [
        "id",
        "createdAt",
        "updatedAt",
        "deletedAt"
      ]
    }
